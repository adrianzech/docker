{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePatterns": [
        "^postgres$",
        "^meilisearch$",
        "^alpine-chrome$",
        "^mongo$"
      ],
      "enabled": false
    }
  ]
}
